{
  "version": 2,
  "builds": [
    { "src": "api/predict.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    { "source": "/", "destination": "/public/index.html" }
  ],
  "headers": [
    {
      "source": "/model/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}